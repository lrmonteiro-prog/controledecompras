<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.controledecompras">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_compra3_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ControleDeCompras"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".main.MainActivityValidade"
            android:exported="true" />
        <activity
            android:name=".lista.ListaActivityValidade"
            android:exported="true" />
        <activity
            android:name=".lista.ComparaPrecoActivityProduto"
            android:exported="true" />
        <activity
            android:name=".lista.ListaActivityProduto"
            android:exported="true" />
        <activity
            android:name=".lista.ListaActivityListaProduto"
            android:exported="true" />
        <activity
            android:name=".main.MainActivityProduto"
            android:exported="true" />
        <activity
            android:name=".main.MainActivity"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>